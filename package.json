{
  "name": "tort",
  "version": "0.0.1",
  "private": true,
  "description": "Адаптивный сайт для кондитерских, продажа тортов",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "8.3.x",
    "browser-sync": "2.23.x",
    "gulp": "3.9.x",
    "gulp-less": "^3.5.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-sourcemaps": "^2.6.4",
    "pixel-glass": "^1.0.13",
    "stylelint": "9.2.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp style",
    "start": "gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "engines": {
    "node": "8.11"
  },
  "dependencies": {
    "eslint": "^5.9.0",
    "normalize.less": "^1.0.0"
  },
  "main": "gulpfile.js",
  "author": "iloi",
  "license": "ISC"
}
